{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": [
          "last 2 chrome versions",
          "last 2 firefox versions",
          "last 2 safari versions",
          "last 2 edge versions",
          "last 2 ios versions"
        ]
      },
      "useBuiltIns": "entry",
      // "debug": true
    }]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import"
  ],

  "env": {
    "test": {
      "presets": [
        "@babel/preset-react",
        ["@babel/preset-env", {
          "modules": "commonjs",
          "targets": {
            "node": "current"
          }
        }]
      ]
    }
  }
}
